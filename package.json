{
  "name": "smultron",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "bun test tests/*.test.ts",
    "test:integration": "bun test tests/integration/*.test.ts",
    "dev": "bun run --watch index.ts",
    "build": "bun build index.ts --outdir ./dist --target node",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth:stage": "cdk synth --context environment=stage",
    "cdk:synth:prod": "cdk synth --context environment=prod",
    "cdk:diff:stage": "cdk diff --context environment=stage",
    "cdk:diff:prod": "cdk diff --context environment=prod",
    "cdk:deploy:stage": "cdk deploy --context environment=stage --require-approval never",
    "cdk:deploy:prod": "cdk deploy --context environment=prod --require-approval never",
    "cdk:destroy:stage": "cdk destroy --context environment=stage --force",
    "cdk:destroy:prod": "cdk destroy --context environment=prod --force"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.637.0",
    "@aws-sdk/lib-dynamodb": "^3.637.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.143",
    "@types/bcryptjs": "^2.4.6",
    "@types/bun": "latest",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.7.0",
    "aws-cdk-lib": "^2.219.0",
    "constructs": "^10.4.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
